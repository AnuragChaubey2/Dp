class Array1{
    public static void main(String[] args){
        
    }
    public static int Building(int n){
        int ob=1;
        int os=1;
        for(int i=2;i<=n;i++){
            int nb=os;
            int ns=os+ob;
            os=ns;
            ob=nb;
        }
        int total=os+ob;
        total=total*total;
        return total;
    }
}
